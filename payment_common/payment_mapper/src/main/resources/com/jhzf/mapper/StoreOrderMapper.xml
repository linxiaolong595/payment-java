<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!--
      mapper为映射的根节点，用来管理DAO接口
      namespace指定DAO接口的完整类名，表示mapper配置文件管理哪个DAO接口(包.接口名)
      mybatis会依据这个接口动态创建一个实现类去实现这个接口，而这个实现类是一个Mapper对象
   -->
<mapper namespace="com.jhzf.mapper.StoreOrderMapper">
    <select id="getStoreDailyOrder" resultType="com.jhzf.pojo.PaymentStore">

    </select>
    <select id="getStoreMonthOrder" resultType="com.jhzf.pojo.PaymentOrder">
        SELECT *
        FROM payment_order
        WHERE order_createtime BETWEEN #{startTime} AND #{endTime} and order_store_id = 1
        ORDER BY order_createtime;
    </select>
</mapper>